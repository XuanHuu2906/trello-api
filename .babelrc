{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current"
      }
    }]
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "asyncGenerators": true,
      "generators": false,
      "async": false
    }],
    ["module-resolver", { 
      "alias": { 
        "~": "./src" 
      } 
    }]
  ]
}